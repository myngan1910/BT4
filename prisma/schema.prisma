generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:12345@localhost:5432/test3?schema=public" 
}

model user {
  id Int @id @default(autoincrement())
  name String
  job String
  image String
  avata String
  description1 String 
  description2 String
  mail String
  phone String

}

model education {
  id Int @id @default(autoincrement())
  year Int
  degree String
  address String
  information String

}

model skills {
 id Int @id @default(autoincrement())
 name String
 percent Int
}

model experience {
  id Int @id @default(autoincrement())
  year Int
  jobl String
  namec String 
  address String
  description String
}

model contact {
 id Int @id @default(autoincrement())
 name String 
 mail String 
 title String 
 message String
}

model profile {
  id Int @id @default(autoincrement())
  name String 
  icon String
  link String
}

model portfolio {
  id Int @id @default(autoincrement())
  name String 
  image String
  link String

}

model clients {
  id Int @id @default(autoincrement())
  name String 
  logo String

}

model social {
  id Int @id @default(autoincrement())
  name String 
  link String
}